{
  "name": "node.js-handson",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "init_db": "call lib/db/start_mongo.bat",
    "reset_db": "node lib/db/install_db.js",

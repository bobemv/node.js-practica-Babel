    "jshint": "jshint . --exclude node_modules/"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cluster": "^0.7.7",
    "cookie-parser": "~1.4.3",
    "crypto": "0.0.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "i18n-nodejs": "^2.0.0",
    "jsonwebtoken": "^7.4.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "jshint": "^2.9.4",
    "mongoose": "^4.9.6"
  }
}
